<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.recommenders.completion.rcp.sessionprocessors">
      <processor
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.calls.rcp.CallCompletionSessionProcessor"
            description="Provides intelligent call proposals, e.g., &quot;99% of all developers called method m() in a similar context.&quot;"
            icon="icons/view16/bullet_green.png"
            id="org.eclipse.recommenders.calls.rcp.completion.processor"
            name="Call Completion Processor"
            preferencePage="org.eclipse.recommenders.calls.rcp.preferences">
      </processor>
   </extension>
   <extension
         point="org.eclipse.equinox.preferences.preferences">
      <initializer
            class="org.eclipse.recommenders.internal.calls.rcp.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.recommenders.injection.modules">
      <module
            class="org.eclipse.recommenders.internal.calls.rcp.CallsRcpModule">
      </module>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.recommenders.completion.rcp.preferences"
            class="org.eclipse.recommenders.internal.calls.rcp.PreferencePage"
            id="org.eclipse.recommenders.calls.rcp.preferences"
            name="Calls">
      </page>
   </extension>
  <extension
         id="org.eclipse.recommenders.calls.rcp.templates.computer"
         name="Adaptive Template Proposals (Recommenders)"
         point="org.eclipse.jdt.ui.javaCompletionProposalComputer">
      <javaCompletionProposalComputer
            activate="true"
            categoryId="org.eclipse.recommenders.calls.rcp.templates.category"
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.calls.rcp.templates.TemplatesCompletionProposalComputer">
      </javaCompletionProposalComputer>
   </extension>
   <extension
         id="org.eclipse.recommenders.calls.rcp.templates.category"
         name="Adaptive Template Proposals (Recommenders)"
         point="org.eclipse.jdt.ui.javaCompletionProposalComputer">
      <proposalCategory
            icon="icons/view16/templates-orange.png">
      </proposalCategory>
   </extension>
   <extension
         point="org.eclipse.recommenders.extdoc.rcp.provider">
      <provider
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.calls.rcp.CallsApidocProvider"
            image="icons/view16/templates-dots.gif"
            name="Call Recommendations">
      </provider>
   </extension>
</plugin>
