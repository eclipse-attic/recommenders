<?xml version="1.0" encoding="UTF-8"?>
    <?eclipse version="3.4"?>
    <plugin>
       <extension
             point="org.eclipse.recommenders.completion.rcp.sessionprocessors">
          <processor
                class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.stats.rcp.StatisticsSessionProcessor"
                icon="icons/elcl16/category.gif"
                id="org.eclipse.recommenders.completion.rcp.processors.statistics"
                name="Statistics">
          </processor>
       </extension>
       <extension
             point="org.eclipse.ui.commands">
          <command
                name="Open Developer Activity Report"
                id="org.eclipse.recommenders.stats.rcp.commands.openstats">
          </command>
       </extension>
       <extension
             point="org.eclipse.ui.handlers">
          <handler
                commandId="org.eclipse.recommenders.stats.rcp.commands.openstats"
                class="org.eclipse.recommenders.stats.rcp.StatisticsDialogHandler">
          </handler>
       </extension>
       <extension
             point="org.eclipse.ui.bindings">
          <key
                commandId="org.eclipse.recommenders.stats.rcp.commands.openstats"
                contextId="org.eclipse.ui.contexts.window"
                sequence="M1+6"
                schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
          </key>
       </extension>
       <extension
             point="org.eclipse.ui.menus">
          <menuContribution
                locationURI="menu:help?after=group.assist">
             <command
                   commandId="org.eclipse.recommenders.stats.rcp.commands.openstats"
                   label="Developer Activity..."
                   style="push">
             </command>
          </menuContribution>
       </extension>
       <extension
             point="org.eclipse.ui.startup">
          <startup
                class="org.eclipse.recommenders.stats.rcp.TriggeredCommandCollector">
          </startup>
       </extension>
    
    </plugin>