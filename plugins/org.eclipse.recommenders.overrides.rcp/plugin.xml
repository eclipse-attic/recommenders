<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.recommenders.completion.rcp.sessionprocessors">
      <processor
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.overrides.rcp.OverrideCompletionSessionProcessor"
            description="Provides intelligent override recommendations, e.g., &quot;94% of all subclasses override method m().&quot;"
            icon="icons/view16/bullet_orange.png"
            id="org.eclipse.recommenders.overrides.rcp.completion.processor"
            name="Override Completion Processor"
            preferencePage="org.eclipse.recommenders.overrides.rcp.preferences">
      </processor>
   </extension>
   <extension
         point="org.eclipse.recommenders.injection.modules">
      <module
            class="org.eclipse.recommenders.internal.overrides.rcp.OverridesRcpModule">
      </module>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.recommenders.completion.rcp.preferences"
            class="org.eclipse.recommenders.internal.overrides.rcp.PreferencePage"
            id="org.eclipse.recommenders.overrides.rcp.preferences"
            name="Overrides">
      </page>
   </extension>
   <extension
         point="org.eclipse.equinox.preferences.preferences">
      <initializer
            class="org.eclipse.recommenders.internal.overrides.rcp.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
