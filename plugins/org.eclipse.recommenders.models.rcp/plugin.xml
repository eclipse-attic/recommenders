<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.recommenders.injection.modules">
      <module
            class="org.eclipse.recommenders.internal.models.rcp.ModelsRcpModule">
      </module>
   </extension>
     <extension
         point="org.eclipse.ui.views">
      <view
            id="org.eclipse.recommenders.models.rcp.views.ModelCoordinatesView"
            icon="icons/view16/depinsp.gif"
            name="Model Coordinates"
            category="org.eclipse.recommenders.rcp.category.views"
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.models.rcp.ModelCoordinatesView">
      </view>
      <view
            id="org.eclipse.recommenders.models.rcp.views.ProjectCoordinatesView"
            name="Project Coordinates"
            icon="icons/view16/depinsp.gif"
            category="org.eclipse.recommenders.rcp.category.views"
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.models.rcp.ProjectCoordinatesView">
      </view>
      <view
            category="org.eclipse.recommenders.rcp.category.views"
            class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.models.rcp.CoordinatesToModelsView"
            icon="icons/view16/depinsp.gif"
            id="org.eclipse.recommenders.models.rcp.CoordinatesToModelsView"
            name="Dependency Overview"
            restorable="true">
      </view>
      
   </extension>
     <extension
           point="org.eclipse.ui.preferencePages">
        <page
              category="org.eclipse.recommenders.rcp.category.preferences"
              class="org.eclipse.recommenders.internal.models.rcp.ModelsPreferencePage"
              id="org.eclipse.recommenders.models.rcp.preferencePages.models"
              name="Models">
        </page>
        <page
              category="org.eclipse.recommenders.rcp.category.preferences"
              class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.models.rcp.AdvisorsPreferencePage"
              id="org.eclipse.recommenders.models.rcp.preferencePages.advisors"
              name="Advisors">
        </page>
     </extension>
     <extension
           point="org.eclipse.core.runtime.preferences">
        <initializer
              class="org.eclipse.recommenders.injection.ExtensionFactory:org.eclipse.recommenders.internal.models.rcp.PreferenceInitializer">
        </initializer>
     </extension>
</plugin>
